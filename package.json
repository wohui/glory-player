{
  "name": "wokay",
  "version": "0.0.1",
  "scripts": {
    "hbuild": "webpack --watch --verbose --color --display-error-details --config ./config/webpack.dev.js ",
    "hstart": "npm run build & node ./server/server.js",
    "hprod": "webpack --mode production --config ./build/webpack.dev.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --profile --config ../config/webpack.dev.js & node ./server/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server",
    "build": "node build/build.js",
    "dev:dll": "cross-env NODE_ENV=development webpack --config build/webpack.dll.conf.js",
    "prod:dll": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.dll.conf.js",
    "dll": "npm run dev:dll && npm run prod:dll"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "cross-env": "^5.1.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "koa-webpack-middleware": "^1.0.7",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.3",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.6.0",
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "element-react": "^1.4.16",
    "element-theme-default": "^1.4.13",
    "es6-promise": "^4.2.4",
    "file-loader": "^1.1.11",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.1",
    "pg": "^7.4.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "precss": "^3.1.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-loadable": "^5.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^7.0.3",
